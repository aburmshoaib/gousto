{
    "name": "ashoaib/gousto",
    "description": "Gousto API PoC",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.2.*",
        "vlucas/phpdotenv": "~2.2",
        "dingo/api": "1.0.x@dev",
        "league/flysystem": " ~1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Gousto\\": "app/Gousto"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gousto\\Tests\\": "tests/Gousto/Tests"
        },
        "classmap": [
            "tests/",
            "database/"
        ]
    }
}
